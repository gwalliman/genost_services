<?xml version="1.0" encoding="utf-8" ?>
<BlockDefinitions>
  <Package name ="DATA">
    <Block>
      <name>INT</name>
      <type>PLUGIN</type>
      <contains>
        <string>INT</string>
        <textbox>INT</textbox>
      </contains>
      <properties>
        <color>239 228 176</color>
        <isConstant>true</isConstant>
        <hasSocks>true</hasSocks>
        <returnType>INT</returnType>
      </properties>
    </Block>
  </Package>
  <Package name="VARIABLES">
    <Block>
      <name>VARIABLE</name>
      <type>PLUGIN</type>
      <contains>
        <string>var</string>
        <string>varName</string>
      </contains>
      <properties>
        <color>255 174 201</color>
        <programOnly>true</programOnly>
        <isCustom>true</isCustom>
      </properties>
    </Block>
  </Package>
  <Package name="PARAMETERS">
    <Block>
      <name>PARAMETER</name>
      <type>PLUGIN</type>
      <contains>
        <string>parType</string>
        <string>parName</string>
      </contains>
      <properties>
        <color>182 220 163</color>
        <programOnly>true</programOnly>
      </properties>
    </Block>
    <Block>
      <name>RETURN</name>
      <type>STATEMENT</type>
      <contains>
        <string>RETURN</string>
        <!--<string>parType</string>-->
        <socket>VARIABLE/METHOD/CONSTANT/PARAMETER</socket>
      </contains>
      <properties>
        <color>182 220 163</color>
        <programOnly>true</programOnly>
      </properties>
    </Block>
  </Package>
  <Package name="METHODS">
    <Block>
      <name>METHOD</name>
      <type>STATEMENT/PLUGIN</type>
      <contains>
        <string>method</string>
        <string>methName</string>
        <parameterList>SOCKET</parameterList>
      </contains>
      <properties>
        <color>182 220 163</color>
        <programOnly>true</programOnly>
        <isCustom>true</isCustom>
        <transformer>true</transformer>
      </properties>
    </Block>
  </Package>
  <Package name="COMPARISON">
    <Block>
      <name>COMPARISON-less</name>
      <!--<name>&#60;</name>-->
      <type>PLUGIN</type>
      <contains>
        <socket>VARIABLE/METHOD/CONSTANT/PARAMETER</socket>
        <string>&#60;</string>
        <!--is less than-->
        <socket>VARIABLE/METHOD/CONSTANT/PARAMETER</socket>
      </contains>
      <properties>
        <color>164 252 242</color>
        <socketsMustMatch>true</socketsMustMatch>
        <intDisabled>false</intDisabled>
        <stringDisabled>true</stringDisabled>
        <booleanDisabled>true</booleanDisabled>
        <isCondition>true</isCondition>
        <hasSocks>true</hasSocks>
        <transformer>true</transformer>
      </properties>
    </Block>
  </Package>
  <Package name="WAIT">
    <Block>
      <name>WAITUNTIL</name>
      <type>STATEMENT</type>
      <contains>
        <string>WAIT UNTIL</string>
        <socket>CONDITION</socket>
      </contains>
      <properties>
        <color>222 153 143</color>
        <hasSocks>true</hasSocks>
        <printLiteral>true</printLiteral>
      </properties>
    </Block>
  </Package>
  <Package name ="ROBOT DATA">
    <Block>
      <name>DIRECTION</name>
      <type>PLUGIN</type>
      <contains>
        <string>DIRECTION</string>
        <dropdown></dropdown>
        <field>FORWARD</field>
        <field>BACKWARD</field>
        <field>FRONT</field>
        <field>REAR</field>
        <field>RIGHT</field>
        <field>LEFT</field>
      </contains>
      <properties>
        <color>239 228 176</color>
        <isRobotConstant>true</isRobotConstant>
        <robotOnly>true</robotOnly>
      </properties>
    </Block>
  </Package>
  <Package name="DRIVE">
    <block>
      <name>DRIVE</name>
      <type>STATEMENT</type>
      <contains>
        <string>DRIVE</string>
        <socket>DIRECTION</socket>
      </contains>
      <properties>
        <color>249 243 127</color>
        <robotOnly>true</robotOnly>
        <hasSocks>true</hasSocks>
      </properties>
    </block>
  </Package>
  <Package name="TURN">
    <block>
      <name>TURNDEGREES</name>
      <type>STATEMENT</type>
      <contains>
        <string>TURN&#13;DEGREES</string>
        <socket>INT</socket>
      </contains>
      <properties>
        <color>249 243 127</color>
        <robotOnly>true</robotOnly>
        <hasSocks>true</hasSocks>
      </properties>
    </block>
  </Package>
  <Package name ="GET DISTANCE">
    <block>
      <name>CHECKRANGE</name>
      <type>PLUGIN</type>
      <contains>
        <string>DISTANCE</string>
        <socket>DIRECTION</socket>
      </contains>
      <properties>
        <color>249 243 127</color>
        <robotOnly>true</robotOnly>
        <isConstant>true</isConstant>
        <hasSocks>true</hasSocks>
        <returnType>INT</returnType>
      </properties>
    </block>
  </Package>
  <Package name ="STOP">
    <block>
      <name>STOP</name>
      <type>STATEMENT</type>
      <contains>
        <string>STOP</string>
      </contains>
      <properties>
        <color>249 243 127</color>
        <robotOnly>true</robotOnly>
      </properties>
    </block>
  </Package>

</BlockDefinitions>